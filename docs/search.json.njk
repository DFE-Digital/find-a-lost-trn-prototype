---
layout: false
permalink: /search.json
eleventyExcludeFromCollections: true
---
[{% for item in collections["search-index"] %}{
  "data": {
    "title": {{ item.data.title | dump | safe }},
    "description": {{ item.data.description | d("") | dump | safe }},
    "section": {{ item.data.eleventyNavigation.parent | d("") | dump | safe }}
  },
  "templateContent": {{ item.templateContent | tokenize | dump | safe }},
  "url": {{ item.url | url | pretty | dump | safe }}
}{% if not loop.last %},{% endif %}{% endfor %}]
